<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="pt-br" lang="pt-br">
  <head>
      
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    
     <script>
      function number_pages() {
        var vars={};
        var x=document.location.search.substring(1).split('&');
        for(var i in x) {var z=x[i].split('=',2);vars[z[0]] = decodeURIComponent(z[1]);}
        var x=['frompage','topage','page','webpage','section','subsection','subsubsection'];
        for(var i in x) {
          var y = document.getElementsByClassName(x[i]);
          for(var j=0; j<y.length; ++j) y[j].textContent = vars[x[i]];
        }
      }
    </script>
    
  </head>
  <body style="margin-top: 70px;" onload="number_pages()">
      
    <table style='font-family:"Courier New", Courier, monospace; font-size:70% ;width:100%'>
      <tr>
        <th width="50%"  align="left"><%= wicked_pdf_image_tag('images/logo3.png') %></th>
        
        <th widht="50%" align= "right">Fecha:  <%= Date.today.strftime("%d/%m/%Y") %>  </th>
        
      </tr>
      <tr>
        <td width="50%" align="left "></td>  
        <td width="50%" align="right">  Page <span class="page"></span> of <span class="topage"></span></td>
      </tr>
    </table>
  </body>
  
  
</html>